        "        if winner==2:\n",
        "            data_X.append(str_to_list(player1))\n",
        "            data_y.append(str_to_list(player2))\n",
        "\n",
        "            score[\"win\"]+=1\n",
        "        else:\n",
        "            score[\"loose\"]+=1\n",
        "\n",
